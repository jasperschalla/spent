{
  "expo": {
    "name": "spent",
    "slug": "spent",
    "scheme": "spent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/spent_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/spent_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#20211e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
